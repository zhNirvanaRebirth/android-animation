<?xml version="1.0" encoding="utf-8"?>
<animated-vector
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt"
    android:drawable="@drawable/bar_vector_drawable">
    <target
        android:name="bar_vector_path" >
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="trimPathStart"
                android:duration="1000"
                android:valueType="floatType"
                android:valueFrom="0"
                android:valueTo="1" />
        </aapt:attr>
    </target>
    <target
        android:name="search_vector_path" >
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="trimPathStart"
                android:duration="1000"
                android:valueType="floatType"
                android:valueFrom="1"
                android:valueTo="0" />
        </aapt:attr>
    </target>
</animated-vector>