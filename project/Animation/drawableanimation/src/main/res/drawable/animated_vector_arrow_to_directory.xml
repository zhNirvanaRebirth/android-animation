<?xml version="1.0" encoding="utf-8"?>
<animated-vector
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt"
    android:drawable="@drawable/arrow_vector_drawable">
    <target
        android:name="arrow_vector_group">
        <aapt:attr name="android:animation">
            <objectAnimator android:propertyName="rotation"
                android:valueFrom="360"
                android:valueTo="0"
                android:valueType="floatType"
                android:duration="1000" />
        </aapt:attr>
    </target>
    <target
        android:name="arrow_vector_path">
        <aapt:attr name="android:animation">
            <objectAnimator android:propertyName="pathData"
                android:duration="1000"
                android:valueType="pathType"
                android:valueTo="M10, 10 L60, 10 L60, 20 L10, 20
                              M10, 30 L60, 30 L60, 40 L10, 40
                              M10, 50 L60, 50 L60, 60 L10, 60"
                android:valueFrom="M10, 35 L50, 10 L50, 20 L20, 35
                              M20, 30 L60, 30 L60, 40 L20, 40
                              M20, 35 L50, 50 L50, 60 L10, 35" />
        </aapt:attr>
    </target>
</animated-vector>